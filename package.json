    "react": "^18",
    "react-dom": "^18",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.30.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/node": "20.19.25",
    "@types/pg": "^8.15.6",
    "@types/react": "18.3.27",
    "@types/react-dom": "^18",
